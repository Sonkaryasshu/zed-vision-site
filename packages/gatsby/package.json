{
  "name": "@zed-vision/zed-vision-site",
  "scripts": {
    "postinstall": "gatsby telemetry --disable",
    "build": "gatsby clean && cp -a ./static/. ./public && gatsby build",
    "start": "yarn generate-certs && yarn gatsby develop --host 0.0.0.0 --https --cert-file .certs/localhost.cert --key-file .certs/localhost.key",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "serve-https": "yarn generate-certs && file_server --cert .certs/localhost.cert --key .certs/localhost.key --host 0.0.0.0 public",
    "generate-certs": "test -d .certs || (mkdir .certs && cd .certs && npx devcert-cli generate localhost)"
  },
  "private": true,
  "description": "Zed vision blog",
  "version": "0.1.0",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@octokit/rest": "18.0.6",
    "gatsby": "2.24.60",
    "uuid": "8.3.0",
    "gatsby-image": "2.4.18",
    "gatsby-plugin-manifest": "2.4.29",
    "gatsby-plugin-mdx": "1.2.40",
    "gatsby-plugin-react-helmet": "3.3.11",
    "gatsby-plugin-sharp": "2.6.36",
    "gatsby-plugin-typography": "2.5.11",
    "gatsby-remark-copy-linked-files": "2.3.15",
    "gatsby-remark-images": "3.3.30",
    "gatsby-remark-responsive-iframe": "2.4.14",
    "gatsby-remark-smartypants": "2.3.11",
    "gatsby-source-filesystem": "2.3.30",
    "gatsby-transformer-sharp": "2.5.15",
    "react": "16.13.1",
    "html2canvas": "1.0.0-rc.7",
    "react-dom": "16.13.1",
    "vis-network": "8.3.2",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-wordpress-2016": "0.16.19",
    "vfile-message": "2.0.4"
  },
  "homepage": "https://www.zed.vision",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zed-vision/zed-vision-site.git"
  },
  "browserslist": {
    "production": [
      "> 5%"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ],
    "ssr": [
      "node 14"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0"
  }
}
