{
  "name": "zed-vision-mono",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:zed-vision/zed-vision-site.git",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "which file_server || deno install --allow-net --allow-read https://deno.land/std@0.67.0/http/file_server.ts",
    "build": "cd packages/gatsby && yarn build",
    "start": "cd packages/gatsby && yarn start",
    "serve": "file_server --cert .certs/localhost.cert --key .certs/localhost.key --host localhost public",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml}\"",
    "lint": "eslint ./gatsby --ext .ts --ext .tsx --fix"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "7.11.5",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@types/mdx-js__react": "1.5.2",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.2",
    "@types/typography": "0.16.3",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-loader": "8.1.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "prettier": "2.1.1",
    "ts-loader": "8.0.3",
    "styled-components": "5.1.1",
    "typescript": "4.0.2"
  }
}
