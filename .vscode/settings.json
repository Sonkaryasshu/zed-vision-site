{
  "files.autoSave": "onFocusChange",
  "editor.folding": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "eslint.enable": true,
  "typescript.autoClosingTags": true,
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.extension.sortImports.sortOnSave": false,
  "typescript.suggest.autoImports": true,
  "typescript.extension.sortImports.quoteStyle": "double",
  "typescript.extension.sortImports.omitSemicolon": true,
  "editor.maxTokenizationLineLength": 60000,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/yarn.lock",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "cSpell.dictionaries": [],
  "cSpell.words": [
    "Erdos",
    "Kanye",
    "Zoltan",
    "ampproject",
    "atrule",
    "browserslist",
    "cacheable",
    "calt",
    "clig",
    "crossorigin",
    "deno",
    "devcert",
    "devcontainer",
    "docgen",
    "downlevel",
    "emotion",
    "esnext",
    "facebox",
    "firstload",
    "focusable",
    "focusring",
    "frontmatter",
    "fullhref",
    "geolocation",
    "gramm",
    "hexcode",
    "iframe",
    "insterted",
    "liga",
    "loader",
    "merriweather",
    "message",
    "mochawesome",
    "mysecretkey",
    "nomodule",
    "octokit",
    "onconnect",
    "parens",
    "precache",
    "precaching",
    "preconnect",
    "prefetched",
    "react",
    "react docgen typescript loader",
    "synthwave",
    "tabindex",
    "textfield",
    "theme",
    "theming",
    "tmpfs",
    "tnum",
    "typeface",
    "typescript",
    "typography",
    "valami",
    "vfile",
    "videostream",
    "wordpress",
    "workerize",
    "xkcd",
    "zoli"
  ],
  "typescript.suggest.paths": false,
  "mdx-preview.build.useSucraseTranspiler": true,
  "mdx-preview.preview.useWhiteBackground": true,
  "mdx-preview.preview.security": "disabled",
}