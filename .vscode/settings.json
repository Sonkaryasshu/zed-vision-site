{
  "files.autoSave": "onFocusChange",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/yarn.lock",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "cSpell.dictionaries": [],
  "cSpell.words": [
    "docgen",
    "downlevel",
    "emotion",
    "frontmatter",
    "iframe",
    "loader",
    "merriweather",
    "message",
    "mochawesome",
    "octokit",
    "prismjs",
    "react",
    "react docgen typescript loader",
    "theme",
    "theming",
    "typeface",
    "typescript",
    "typography",
    "vfile",
    "wordpress"
  ],
  "typescript.suggest.paths": false,
  "mdx-preview.build.useSucraseTranspiler": true,
  "mdx-preview.preview.useWhiteBackground": true,
  "mdx-preview.preview.security": "disabled"
}
